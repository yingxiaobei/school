  "scripts": {
    "start": "react-app-rewired start",
    "check-commit": "node ./scripts/check-commit",
    "test-all": "sh -e ./scripts/test-all.sh",
    "server": "json-server --watch src/mocks/db.json",
    "start:yantai": "env-cmd -f .yantai.env.development react-app-rewired start",
    "start:wenzhou": "env-cmd -f .wenzhou.env.development react-app-rewired start",
    "start:kedu": "env-cmd -f .kedu.env.development react-app-rewired start",
    "build": "react-app-rewired build",
    "build:yantai": "env-cmd -f .yantai.env.production react-app-rewired build",
    "build:yantai-test": "env-cmd -f .yantai.env.test react-app-rewired build",
    "build:wenzhou": "env-cmd -f .wenzhou.env.production react-app-rewired build",
    "build:wenzhou-test": "env-cmd -f .wenzhou.env.test react-app-rewired build",
    "build:kedu": "env-cmd -f .kedu.env.production react-app-rewired build",
    "test": "react-app-rewired test",
    "build:dev": "env-cmd -f .env.development react-app-rewired build",
    "deploy:dev": "docker-compose -f docker-compose.dev.yml up -d --build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "menu": "node ./scripts/generate-menus",
    "page": "node ./scripts/generate-page",
    "deploy:school-1445": "sh -e ./scripts/deploy-school-1445.sh",
    "deploy:yantai-1445": "sh -e ./scripts/deploy-yantai-1445.sh",
    "deploy:publicService-1445": "sh -e ./scripts/deploy-publicService-1445.sh",
    "deploy:kedu-1445": "sh -e ./scripts/deploy-kedu-1445.sh",
    "gen-routers": "node ./scripts/generate-router-all",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "camelcase": "off",
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/camelcase": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-empty-interface": "off",
      "@typescript-eslint/ban-types": "off",
      "eqeqeq": "off",
      "no-unreachable": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
